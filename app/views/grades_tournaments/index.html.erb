<p id="notice"><%= notice %></p>

<h1>Grades Tournaments</h1>

<table>
  <thead>
    <tr>
      <th>Grades</th>
      <th>Tournament</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @grades_tournaments.each do |grades_tournament| %>
      <tr>
        <td><%= grades_tournament.grades %></td>
        <td><%= grades_tournament.tournament %></td>
        <td><%= link_to 'Show', grades_tournament %></td>
        <td><%= link_to 'Edit', edit_grades_tournament_path(grades_tournament) %></td>
        <td><%= link_to 'Destroy', grades_tournament, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Grades Tournament', new_grades_tournament_path %>
